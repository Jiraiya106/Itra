{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
    {
    "name": "nginx",
    "image": "225050420367.dkr.ecr.us-west-2.amazonaws.com/task:nginxpy1",
    "memory": "256",
    "cpu": "256",
    "essential": true,
    "portMappings": [
    {
    "containerPort": "80",
    "protocol": "tcp"
    }
    ],
    "links": ["app"]
    },
    {
    "name": "app",
    "image": "225050420367.dkr.ecr.us-west-2.amazonaws.com/task:apppy",
    "memory": "256",
    "cpu": "256",
    "essential": true
    }
    ],
    "volumes": [],
    "networkMode": "bridge",
    "placementConstraints": [],
    "family": "application-stack"
}