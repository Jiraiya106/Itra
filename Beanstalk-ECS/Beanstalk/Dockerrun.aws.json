{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "app",
        "image": "225050420367.dkr.ecr.us-west-2.amazonaws.com/task:apppy",
        "hostname": "app",
        "essential": true,
        "portMappings": [
            {
              "hostPort": 8000,
              "containerPort": 8000
            }
          ],
        "memoryReservation": 600
      },
      {
        "name": "nginxbean",
        "image": "225050420367.dkr.ecr.us-west-2.amazonaws.com/task:nginxpy1",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["app"],
        "memoryReservation": 128
      }
    ]
}