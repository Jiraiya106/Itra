{
    "requiresCompatibilities": [
      "EC2"
    ],
    "containerDefinitions": [
      {
        "name": "app",
        "image": "225050420367.dkr.ecr.us-west-2.amazonaws.com/task:apppy",
        "memoryReservation": 600,
        "cpu": 0,
        "essential": true
      },
      {
        "name": "nginx",
        "image": "225050420367.dkr.ecr.us-west-2.amazonaws.com/task:nginxpy1",
        "memory": 128,
        "cpu": 0,
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80,
            "protocol": "tcp"
          }
        ],
        "links": [
            "app"
          ]
      }
    ],
    "volumes": [],
    "networkMode": "bridge",
    "placementConstraints": [],
    "family": "nginxnew"
  }